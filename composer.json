{
    "name": "whirlwind-framework/adapter-router-league",
    "description": "Whirlwind framework adapter for league/route, laminas/laminas-diactoros and laminas/laminas-httphandlerrunner",
    "keywords": [
        "router request response adapter"
    ],
    "authors": [
        {
            "name": "Alexander Stepanenko",
            "email": "alex.stepanenko@gmail.com"
        }
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "require": {
        "php": "~7.4||>=8.0",
        "ext-json": "*",
        "league/route": "5.*",
        "laminas/laminas-diactoros": "~2.10.0",
        "laminas/laminas-httphandlerrunner": "2.*",
        "whirlwind-framework/framework": "*",
        "league/container": "4.2.0"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.2",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Whirlwind\\Adapter\\League\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests"
        }
    }
}
